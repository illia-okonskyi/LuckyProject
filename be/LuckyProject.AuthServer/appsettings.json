{
  "Application": {
    "Version": {
      "FilePath": "config/version.txt",
      "IsEncrypted": true
    },
    "Db": {
      "DbDirectory": "db",
      "ConnectionString": "Data Source=db/AuthServer.db; Foreign Keys=True"
    },
    "Localization": {
      "I18nDir": "i18n"
    },
    "Sessions": {
      "ExpireInterval": "0:5:0"
    },
    "WebServer": {
      "PublicName": "localhost",
      "Port": 5000,
      "CertPath": "config/cert.pfx",
      "LpApiOrigins": [
        "https://localhost:5001"
      ]
    }
  }
}
